{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "localStorage": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": "error",
    "prettier/prettier": "error",
    "camelcase": "warn",
    "prefer-const": "off",
    "react/prop-types": "off"
  }
}
